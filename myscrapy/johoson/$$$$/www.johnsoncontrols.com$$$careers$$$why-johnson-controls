<script type="text/javascript">
var disclaimer = document.getElementsByClassName('container page-sub');
disclaimer[1].id = "footer-disclaimer";
var disclaimerDiv = document.getElementById('footer-disclaimer').childNodes[1];
disclaimerDiv.className = "container content-text";
var imageCallouts = document.getElementsByClassName('image-callout col-sm-3');
imageCallouts[0].id = "callout1";
imageCallouts[1].id = "callout2";
imageCallouts[2].id = "callout3";
var callout1 = document.getElementById('callout1');
var callout2 = document.getElementById('callout2');
var callout3 = document.getElementById('callout3');
callout1.className = "image-callout col-sm-4";
callout2.className = "image-callout col-sm-4";
callout3.className = "image-callout col-sm-4";

var sectionThrees = document.getElementsByClassName('revised-image-callout-right revised-image-callout revised-callout-wrapper container');
sectionThrees[1].id= "section2";
var section2 = document.getElementById('section2').childNodes;
var section2Text = section2[3];
var section2Image = section2[5];
section2Text.id = "section2-text";
section2Image.id = "section2-image";

var aboutTitle = document.querySelector('.en .callout-container [href*="/about-us/our-company"] .image-title');
aboutTitle.innerText = "About Us";


// Responsive image positioning
var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
var max_w = 767;
if(w <= max_w){
  section2Image.parentNode.insertBefore(section2Image, section2Text);
}
window.addEventListener('resize', function(event){
  w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
  if(w <= max_w){
  	section2Image.parentNode.insertBefore(section2Image, section2Text);
	} else {
    section2Text.parentNode.insertBefore(section2Text, section2Image);
  }
});
</script>
    

