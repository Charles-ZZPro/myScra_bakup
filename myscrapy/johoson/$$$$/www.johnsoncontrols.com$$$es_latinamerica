        var autoplay = false;

        function DetectAutoplay() {
            var backgroundVideo = document.getElementById("home-background-video");
            backgroundVideo.addEventListener('play', function () {
                backgroundVideo.style.display = "block";
                autoplay = true;
            }, false);
